{
    "static": {
        "toWatch": "node_modules/govuk-frontend/govuk/assets/",
        "toDelete": "accessibility_monitoring_platform/static/compiled/assets/",
        "dest": "accessibility_monitoring_platform/static/compiled/"
    },
    "static_img": {
        "toWatch": "accessibility_monitoring_platform/static/img/",
        "toDelete": "accessibility_monitoring_platform/static/compiled/img/",
        "dest": "accessibility_monitoring_platform/static/compiled/"
    },
    "css": {
        "toWatch": "accessibility_monitoring_platform/static/scss/",
        "access_point": "accessibility_monitoring_platform/static/scss/init.scss",
        "dest": "accessibility_monitoring_platform/static/compiled/css/"
    },
    "js": {
        "toWatch": "accessibility_monitoring_platform/static/js",
        "dest": "accessibility_monitoring_platform/static/compiled/js"
    }
}